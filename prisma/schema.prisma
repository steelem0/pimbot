generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model Session {
  id          String   @id @default(uuid())
  createdAt   DateTime @default(now())
  consented   Boolean
  userEmail   String?
  goal        String?
  responses   Json
  pdfPath     String?
}
